      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product_type"
    },
    "landingSection": {
      "type": "component",
      "repeatable": false,
      "component": "reusable.hero"
    },
    "meta_data": {
      "type": "component",
      "repeatable": false,
      "component": "reusable.meta-fields"
    },
