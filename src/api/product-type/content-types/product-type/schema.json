{
  "kind": "collectionType",
  "collectionName": "product_types",
  "info": {
    "singularName": "product-type",
    "pluralName": "product-types",
    "displayName": "Product Type",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
